    <script>
    var _global = {
        fn: {
            init: function() {
                this.tab();
            },
            tab: function() {
                var $tabgroups = $('.tabcont').find('.group'),
                    $tabs = $('.tab').find('li');
                $('.tab').on('click', 'li', function() {
                    var idx = $(this).index();
                    $(this).addClass('current').siblings().removeClass('current');
                    $tabgroups.eq(idx).show().siblings().hide();
                })

                $tabs.eq(0).addClass('current');
                $tabgroups.eq(0).show();
            }
        }
    }
    $(function(){
        _global.fn.init();
    })
    </script>
